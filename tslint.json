{
  "defaultSeverity": "error",
  "extends": ["tslint:latest"],
  "jsRules": {},
  "rules": {
    "max-line-length": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "no-empty-interface": false,
    "eofline": true,
    "no-empty": false,
    "ban-types": false,
    "prefer-for-of": false,
    "no-bitwise": false,
    "no-console": false,
    "arrow-parens": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "singleline": "never",
        "esSpecCompliant": true
      }
    ],
    "semicolon": [true, "always", "ignore-bound-class-methods"]
  },
  "rulesDirectory": []
}
